CREATE SET TABLE TRACKINGDB.LD_TBL_RUN_STATS ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO
     (
      SUBJECT_AREA VARCHAR(64) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'SUBJECT_AREA' NOT NULL,
      TGT_DB VARCHAR(128) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'TGT_DB' NOT NULL,
      TGT_TBL_NM VARCHAR(128) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'TGT_TBL_NM' NOT NULL,
      LD_FILE_NAME VARCHAR(512) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'LD_FILE_NAME',
      RUN_SEQ DECIMAL(5,0) TITLE 'RUN_SEQ' NOT NULL,
      RTN_CD VARCHAR(5) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'RTN_CD' NOT NULL,
      RTN_STAT_CD VARCHAR(5) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'RTN_STAT_CD',
      START_DT_TS TIMESTAMP(0) TITLE 'START_DT_TS',
      END_DT_TS TIMESTAMP(0) TITLE 'END_DT_TS',
      TOT_REC_READ DECIMAL(10,0) TITLE 'TOT_REC_READ',
      TOT_REC_REJ DECIMAL(10,0) TITLE 'TOT_REC_REJ',
      TOT_REC_INS DECIMAL(10,0) TITLE 'TOT_REC_INS',
      TOT_REC_UPD DECIMAL(10,0) TITLE 'TOT_REC_UPD',
      TOT_REC_DEL DECIMAL(10,0) TITLE 'TOT_REC_DEL',
      TOT_REC_UV DECIMAL(10,0) TITLE 'TOT_REC_UV',
      TOT_REC_ET DECIMAL(10,0) TITLE 'TOT_REC_ET',
      TOT_REC_DUP DECIMAL(10,0) TITLE 'TOT_REC_DUP',
      FILE_MIN_DT_TS TIMESTAMP(0) TITLE 'FILE_MIN_DT_TS', 
      FILE_MAX_DT_TS TIMESTAMP(0) TITLE 'FILE_MAX_DT_TS', 
	  FILE_DISTINCT_COUNT DECIMAL(10,0) TITLE 'FILE_DISTINCT_COUNT',
      CRT_DT_TS TIMESTAMP(0) TITLE 'CRT_DT_TS',
      LAST_UPD_DT_TS TIMESTAMP(0) TITLE 'LAST_UPD_DT_TS',
      USER_ID VARCHAR(30) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'USER_ID')

PRIMARY INDEX ( TGT_DB ,TGT_TBL_NM );
